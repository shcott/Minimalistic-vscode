{
	"name": "Minimalistic",
	"type": "dark",
	"colors": {
		"foreground": "#dfdfdf",
		"focusBorder": "#222",
		"widget.shadow": "#0000",
		
		"statusBar.background": "#222",
		"statusBar.border": "#333",
		"statusBar.foreground": "#dfdfdf",
		"statusBar.debuggingBackground": "#77a",
		"statusBar.debuggingForeground": "#eee",
		"statusBar.noFolderBackground": "#aaa",
		"statusBar.noFolderForeground": "#111",
		
		"panel.background": "#222",
		"panel.border": "#333",
		"panelTitle.activeBorder": "#dfdfdf",
		"panelTitle.activeForeground": "#dfdfdf",
		"panelTitle.inactiveForeground": "#777",
		
		"sideBar.background": "#222",
		"sideBar.foreground": "#dfdfdf",
		"sideBar.border": "#333",
		"sideBarTitle.foreground": "#aaa",
		"sideBarSectionHeader.background": "#353535",
		"sideBarSectionHeader.foreground": "#dfdfdf",
		
		"activityBar.background": "#353535",
		"activityBar.foreground": "#dfdfdf",
		
		"tab.activeBackground": "#333",
		"tab.activeForeground": "#dfdfdf",
		"tab.activeBorder": "#dfdfdf",
		"tab.inactiveBackground": "#232323",
		"tab.hoverBackground": "#444",
		
		"dropdown.background": "#1a1a1a",
		"dropdown.border": "#101010",
		
		"button.background": "#aaa",
		"button.foreground": "#111",
		
		"editor.background": "#202022",
		"editor.foreground": "#dfdfdf",
		
		"editor.selectionBackground": "#3c3c3c",
		
		"editor.selectionHighlightBackground": "#303030",
		"editor.selectionHighlightBorder": "#202022",
		
		"editor.lineHighlightBorder": "#202022",
		"editor.lineHighlightBackground": "#272729",
		
		"editorCursor.background": "#111",
		"editorCursor.foreground": "#dfdfdf",
		
		"editorLineNumber.activeForeground": "#aaa",
		"editorLineNumber.foreground": "#444",
		"editorWhitespace.foreground": "#333",
		"editorIndentGuide.background": "#333",
		
		"editorBracketMatch.background": "#3c3c3c",
		"editorBracketMatch.border": "#3c3c3c",
		
		"editor.findMatchBackground": "#444",
		"editor.findMatchHighlightBackground": "#333",
		
		"editorGroupHeader.tabsBackground": "#222",
		"editorGroup.dropBackground": "#333a",
		"editorGroup.border": "#333"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment"
			],
			"settings": {
				// "foreground": "#d2a562",
				"foreground": "#96ddab",
				"fontStyle": ""
			}
		},
		{
			"name": "Normal",
			"scope": [
				"variable",
				"entity.name.function",
				"meta.function",
				"meta.function-call",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#dfdfdf",
				"fontStyle": ""
			}
		},
		{
			"name": "Darker",
			"scope": [
				"keyword",
				"keyword.control",
				"keyword.other",
				"storage.type",
				"storage.modifier",
				"support.type.sys-types",
				"support.type.posix-reserved",
				"support.type.stdint",
				"constant.language",
				
				"keyword.operator",
				"keyword.operator.logical",
				"keyword.operator.arithmetic",
				"keyword.operator.assignment",
				"keyword.operator.new",
				"punctuation.separator",
				
				"support.variable.glsl"
			],
			"settings": {
				"foreground": "#aaa",
				"fontStyle": ""
			}
		},
		{
			"name": "Purple",
			"scope": [
				"string",
				"constant.numeric"
			],
			"settings": {
				"foreground": "#ada0f8",
				// "foreground": "#e1d17f",
				"fontStyle": ""
			}
		},
		{
			"name": "Dark Purple",
			"scope": [
				"meta.preprocessor",
				"meta.preprocessor.include",
				"meta.preprocessor.macro",
				"keyword.control.directive.conditional",
				"keyword.control.directive.include",
				"keyword.control.directive.define",
				"keyword.control.directive.undef",
				"keyword.control.directive.endif",
				"keyword.control.directive.pragma",
				"constant.character.escape.line-continuation",
				"meta.preprocessor.pragma",
				"keyword.control.hlsl",
				
				"markup.heading"
			],
			"settings": {
				"foreground": "#77a",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"invalid"
			],
			"settings": {
				"foreground": "#E32",
				"fontStyle": ""
			}
		},
		
		{
			"name": "Markup Plaintext",
			"scope": [
				"meta.paragraph.markdown"
			],
			"settings": {
				"foreground": "#dfdfdf",
				"fontStyle": ""
			}
		},
		{
			"name": "Markup Bold",
			"scope": [
				"markup.bold"
			],
			"settings": {
				"foreground": "#aaa",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup Italics",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"foreground": "#aaa",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Makeup Code",
			"scope": [
				"markup.inline.raw",
				"markup.raw"
			],
			"settings": {
				"foreground": "#ada0f8",
				"fontStyle": ""
			}
		},
		{
			"name": "Markup List",
			"scope": [
				"markup.list",
				"beginning.punctuation.definition.list.markdown"
			],
			"settings": {
				"foreground": "#aaa",
				"fontStyle": ""
			}
		}
	]
}
